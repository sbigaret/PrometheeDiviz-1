<?xml version='1.0' encoding='utf-8'?>
<program_description>
  <program provider="PUT" name="PROMETHEE-I_ranking" version="1.0.0" displayName="PROMETHEE-I_ranking" />
  <documentation>
    <description>This module is calculating ranking using Promethee I method. The result is shown as the table of outranking relationship, where the relationship is a weak preference.</description>
    <contact><![CDATA[Magdalena Dzi?cielska <magdalenadziecielska6@gmail.com>]]></contact>
    <url>https://github.com/MagdalenaDziecielska/PrometheeDiviz</url>
  </documentation>
  <parameters>

    <input id="input1" name="alternatives" displayName="alternatives" isoptional="0">
      <documentation>
        <description>Alternatives to consider.</description>
      </documentation>
      <xmcda tag="alternatives" />
    </input>

    <input id="input2" name="positive_flow" displayName="positive flows" isoptional="0">
      <documentation>
        <description>Positive flows of given alternatives and profiles.</description>
      </documentation>
      <xmcda tag="alternativesValues" />
    </input>

    <input id="input3" name="negative_flow" displayName="negative flows" isoptional="0">
      <documentation>
        <description>Negative flows of given alternatives and profiles.</description>
      </documentation>
      <xmcda tag="alternativesValues" />
    </input>

    <output id="output1" name="ranking" displayName="ranking">
      <documentation>
        <description>Result computed from the given data.</description>
      </documentation>
      <xmcda tag="alternativesComparisons" />
    </output>

    <output id="output2" name="messages" displayName="messages">
      <documentation>
        <description>Messages or errors generated by this module.</description>
      </documentation>
      <xmcda tag="methodMessages" />
    </output>

  </parameters>
</program_description>
