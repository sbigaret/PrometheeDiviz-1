<?xml version='1.0' encoding='utf-8'?>
<program_description>
  <program provider="PUT" name="PROMETHEE-II_flows" version="1.0.0" displayName="PROMETHEE-II_flows" />
  <documentation>
    <description>Final flows calculated with Promethee II method as a difference of positive and negative outranking flows.</description>
    <contact><![CDATA[Magdalena Dzi?cielska <magdalenadziecielska6@gmail.com>]]></contact>
    <url>https://github.com/MagdalenaDziecielska/PrometheeDiviz</url>
    <reference>J.-P. Brans and B. Mareschal, 2005. PROMETHEE methods. In J. Figueira, S. Greco, M. Ehrgott, editors, Multiple Criteria Decision Analysis. State of the Art Surveys, 163â€“195. Springer, New York, USA.</reference>
  </documentation>
  <parameters>

    <input id="input1" name="alternatives" displayName="alternatives" isoptional="0">
      <documentation>
        <description>Alternatives to consider.</description>
      </documentation>
      <xmcda tag="alternatives" />
    </input>

    <input id="input2" name="positive_flow" displayName="positive flows" isoptional="0">
      <documentation>
        <description>Positive flows of given alternatives and profiles.</description>
      </documentation>
      <xmcda tag="alternativesValues" />
    </input>

    <input id="input3" name="negative_flow" displayName="negative flows" isoptional="0">
      <documentation>
        <description>Negative flows of given alternatives and profiles.</description>
      </documentation>
      <xmcda tag="alternativesValues" />
    </input>

    <input id="input5" name="categories_profiles" displayName="categories profiles" isoptional="1">
      <documentation>
        <description>Definitions of profiles (boundary or central) which should be used for classes (categories) representation.</description>
      </documentation>
      <xmcda tag="categoriesProfiles" />
    </input>

    <input id="input4" name="parameters" displayName="method parameters" isoptional="0">
      <documentation>
        <description>Parameter specifies the type of elements provided for comparison.
          Choosing 'boundary_profiles' or 'central_profiles' requires providing input file: 'categories_profiles'.</description>
      </documentation>
      <xmcda tag="programParameters"><![CDATA[
        
		<programParameters>
			<parameter id="comparison_with" name="comparison_with">
				<values>
					<value>
						<label>%1</label>
					</value>
				</values>
			</parameter>
		</programParameters>
        
      ]]></xmcda>
      <gui status="preferGUI">
        <entry id="%1" type="enum" displayName="comparison with">
          <items>
            <item id="item0">
              <description>alternatives vs alternatives</description>
              <value>alternatives</value>
            </item>
            <item id="item1">
              <description>alternatives vs boundary profiles</description>
              <value>boundary_profiles</value>
            </item>
            <item id="item2">
              <description>alternatives vs central profiles</description>
              <value>central_profiles</value>
            </item>
          </items>
          <defaultValue>item0</defaultValue>
        </entry>
      </gui>
    </input>

    <output id="output1" name="flows" displayName="flows">
      <documentation>
        <description>Result computed from the given data.</description>
      </documentation>
      <xmcda tag="alternativesValues" />
    </output>

    <output id="output2" name="messages" displayName="messages">
      <documentation>
        <description>Messages or errors generated by this module.</description>
      </documentation>
      <xmcda tag="programExecutionResult" />
    </output>

  </parameters>
</program_description>
